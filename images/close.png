                data: {
                    rowid: id,
                },
                success: function(data) {
                    $(`#modalCartPriceProduct-${id}`).text(`Rp. ${number_format(data).split(",").join(".")}`);
                    $(`input#qtyProductCart-${id}`).val(inputJml);
                }
            })
        }
    }

