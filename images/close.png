    // plus minus qty cart
    function minusProductCart(id) {
        let inputJml;
        inputJml = parseInt($(`input#qtyProductCart-${id}`).val());
        inputJml = inputJml - 1;
        if (inputJml >= 1) {
            $(`input#qtyProductCart-${id}`).val("..");
            $.ajax({
                url: "https://kitadigital.my.id/cart/edit_qty_cart?type=minus",
